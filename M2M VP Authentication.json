{
    "openapi": "3.0.2",
    "info": {
        "title": "M2M VP Authentication",
        "version": "0.1",
        "description": "This specification is draft specification for iSHARE specifications updated with compliance to W3C based VC-Model as specified here https://www.w3.org/TR/2022/REC-vc-data-model-20220303/\nAdditionally, it has been made complaint with EBSI specifications defined at: https://api-pilot.ebsi.eu/docs/apis\n",
        "contact": {

        },
        "license": {
            "name": "GNU AGPLv3",
            "url": "https://www.gnu.org/licenses/agpl.txt"
        }
    },
    "paths": {
        "/token_vp": {
            "post": {
                "requestBody": {
                    "description": "The verifialble presentation including the verifiable credentials",
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/vp"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/access_token"
                                }
                            }
                        },
                        "description": "When authentication is successful a token is returned"
                    },
                    "400": {
                        "description": "When request is not correct, 400 is returned"
                    },
                    "401": {
                        "description": "When the validation fails 401 is returned"
                    }
                },
                "description": "This endpoint allows a participant to present a Verifable Presentation to get an access token. The access token recevied is complaint with iSHARE framework and can be used in with other iSHARE participants."
            }
        }
    },
    "components": {
        "schemas": {
            "vp_token": {
                "description": "Verifiable presentation as specified \"vp\" as a signed JWT",
                "type": "object"
            },
            "vp": {
                "title": "Root Type for vp",
                "description": "    ",
                "required": [
                    "@context",
                    "type",
                    "verifiableCredential",
                    "proof"
                ],
                "type": "object",
                "properties": {
                    "@context": {
                        "description": "The context to be set. The credential and presentation are based W3C standard VC-Model. So \"https://www.w3.org/2018/credentials/v1\" is mandatory. Additional contexts could be added as necessary.",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "type": {
                        "description": "MUST be \"VerifiablePresentation\"",
                        "type": "string",
                        "example": "\"VerifiablePresentation\""
                    },
                    "verifiableCredential": {
                        "description": "Verifiable credentials issued by issuer which can be verified in this context.",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/verifiableCredential"
                        }
                    },
                    "proof": {
                        "$ref": "#/components/schemas/proof_vp",
                        "description": "Digital proof that makes this credential tamper-evident.",
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "created": {
                                "format": "date-time",
                                "type": "string"
                            },
                            "proofPurpose": {
                                "type": "string"
                            },
                            "verificationMethod": {
                                "type": "string"
                            },
                            "challenge": {
                                "type": "string"
                            },
                            "domain": {
                                "type": "string"
                            },
                            "jws": {
                                "type": "string"
                            }
                        }
                    }
                },
                "example": {
                    "@context": [
                        "https://www.w3.org/2018/credentials/v1",
                        "https://www.w3.org/2018/credentials/examples/v1"
                    ],
                    "type": "VerifiablePresentation",
                    "verifiableCredential": [
